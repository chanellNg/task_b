functions:
  index:
    handler: quoteController.index
    events:
      - http: 
          path: quotes
          method: get
          cors: true
  new:
    handler: quoteController.new
    events:
      - http: 
          path: quotes
          method: post
          cors: true 
  view:
    handler: quoteController.view
    events:
      - http: 
          path: quotes/:quote_id
          method: get
          cors: true
  update:  
    handler: quoteController.update
    events:
      - http: 
          path: quotes
          method: patch
          cors: true  
      - http: 
          path: quotes
          method: put
          cors: true 
  delete:  
    handler: quoteController.delete
    events:
      - http: 
          path: quotes
          method: delete
          cors: true  